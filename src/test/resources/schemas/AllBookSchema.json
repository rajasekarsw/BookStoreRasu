{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Books",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": { "type": "string" },
      "published_year": { "type": "integer" },
      "author": { "type": "string" },
      "id": { "type": "integer" },
      "book_summary": { "type": "string" }
    },
    "required": ["name", "published_year", "author", "id", "book_summary"],
    "additionalProperties": false
  }
}
